rfile.css'\nimport { ProfileStatus } from '../components/ProfileStatus'\n\ntype PathParamsType = {\n    userId: string\n}\n\nexport const ProfilePage: React.FC<RouteComponentProps<PathParamsType>> = (props) => {\n    const {id, profile, status, userLoggedIn} = useSelector((state: RootState) => ({\n        id: state.auth.userData.id,\n        profile: state.profile.profile,\n        status: state.profile.status,\n        userLoggedIn: state.auth.userLoggedIn,\n    }))\n\n    const {userId} = props.match.params\n    const dispatch = useDispatch()\n\n    useEffect(() => {\n        console.log('Profile render')\n        if (userId) {\n            dispatch(fetchProfile(+userId))\n            dispatch(fetchStatus(+userId))\n        }\n        if (!userId && id !== null) {\n            dispatch(fetchProfile(id))\n            dispatch(fetchStatus(id))\n        }\n    }, [id, userId])\n\n    if (!userLoggedIn) {\n        return <Redirect to='/login'/>\n    }\n\n    return (\n        <div className='Profile-page'>\n            <div className='Profile-page__avatar'>\n                {\n                    (profile && profile.photos.small !== null)\n                        ? <img src={profile.photos.small} alt=''/>\n                        : <img src={svgLogo} alt=''/>\n                }\n            </div>\n            <div className='Profile-page__login'>{profile?.fullName}</div>\n            <ProfileStatus status={status}/>\n        </div>\n    )\n}\n","/Users/kocherg1n/Main/WEB/workspace/social-network-ts/src/components/Header.tsx",[],"/Users/kocherg1n/Main/WEB/workspace/social-network-ts/src/redux/middleware.ts",[],"/Users/kocherg1n/Main/WEB/workspace/social-network-ts/src/components/Layout.tsx",[],"/Users/kocherg1n/Main/WEB/workspace/social-network-ts/src/redux/store/reducers/profileReducer.ts",[],"/Users/kocherg1n/Main/WEB/workspace/social-network-ts/src/index.tsx",[],"/Users/kocherg1n/Main/WEB/workspace/social-network-ts/src/components/ProfileStatus.tsx",[],"/Users/koch